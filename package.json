{
  "name": "gtfs-ai-generator",
  "version": "1.0.0",
  "description": "Generador de datos GTFS usando IA y MongoDB",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "dev:local": "nodemon src/app.js --env-file=.env",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "setup": "powershell -ExecutionPolicy Bypass -File scripts/dev-setup.ps1",
    "db:queries": "node scripts/mongodb-queries.js",
    "db:admin": "node scripts/mongodb-admin.js",
    "db:clear": "node scripts/mongodb-admin.js clear",
    "db:backup": "node scripts/mongodb-admin.js backup",
    "db:optimize": "node scripts/mongodb-admin.js optimize",
    "docker:build": "docker build -t gtfs-ai-generator .",
    "docker:run": "docker run -p 3000:3000 gtfs-ai-generator",
    "docker:compose": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:restart": "docker-compose restart app"
  },
  "keywords": [
    "gtfs",
    "ai",
    "mongodb",
    "transit",
    "transportation",
    "openai"
  ],
  "author": "Tu Nombre",
  "license": "MIT",
  "dependencies": {
    "@langchain/community": "^0.0.20",
    "@langchain/core": "^0.1.0",
    "@turf/distance": "^7.2.0",
    "@turf/helpers": "^7.2.0",
    "archiver": "^6.0.1",
    "axios": "^1.6.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "geolib": "^3.3.4",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "langchain": "^0.1.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "mongoose": "^8.0.3",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "openai": "^4.20.1",
    "polyline": "^0.2.0",
    "turf": "^3.0.14",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tu-usuario/gtfs-ai-generator.git"
  },
  "bugs": {
    "url": "https://github.com/tu-usuario/gtfs-ai-generator/issues"
  },
  "homepage": "https://github.com/tu-usuario/gtfs-ai-generator#readme"
}
